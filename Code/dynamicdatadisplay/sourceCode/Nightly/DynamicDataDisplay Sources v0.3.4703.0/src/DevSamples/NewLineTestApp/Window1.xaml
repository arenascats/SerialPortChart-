<Window x:Class="NewLineTestApp.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
        Title="DynamicDataDisplay Sample" WindowState="Maximized"
        Icon="{x:Static d3:D3IconHelper.DynamicDataDisplayWhiteIcon}">
    <Grid>
        <DockPanel>
            <Slider DockPanel.Dock="Bottom" Minimum="0" Maximum="190" ValueChanged="slider_ValueChanged" AutoToolTipPlacement="BottomRight" 
                    TickPlacement="BottomRight" Value="181" Name="slider"/>
            <d3:ChartPlotter Name="plotter" MainHorizontalAxisVisibility="Collapsed" MainVerticalAxisVisibility="Collapsed">
            </d3:ChartPlotter>
        </DockPanel>
    </Grid>
</Window>
