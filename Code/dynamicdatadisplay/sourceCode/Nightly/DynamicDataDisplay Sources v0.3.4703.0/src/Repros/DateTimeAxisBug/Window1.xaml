<Window x:Class="DateTimeAxisBug.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
        Title="DynamicDataDisplay Sample" WindowState="Maximized"
        Icon="{x:Static d3:D3IconHelper.DynamicDataDisplayWhiteIcon}">
    <Grid>
        <d3:ChartPlotter Name="plotter">
            <d3:ChartPlotter.MainHorizontalAxis>
                <d3:HorizontalDateTimeAxis/>
            </d3:ChartPlotter.MainHorizontalAxis>
			<d3:ChartPlotter.MainVerticalAxis>
				<d3:VerticalDateTimeAxis/>
			</d3:ChartPlotter.MainVerticalAxis>
        </d3:ChartPlotter>
    </Grid>
</Window>
