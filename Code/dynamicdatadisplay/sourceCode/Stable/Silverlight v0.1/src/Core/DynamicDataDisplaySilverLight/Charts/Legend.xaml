<UserControl x:Class="Microsoft.Research.DynamicDataDisplay.Charts.Legend"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid x:Name="LayoutRoot" Background="Transparent">
        <Rectangle HorizontalAlignment="Left" VerticalAlignment="Top" Width="auto" Height="auto" Name="boundingRect" RadiusX="5" RadiusY="5" Stroke="Gray" Fill="#BBFFFFFF">
        </Rectangle>
        <StackPanel HorizontalAlignment="Left" VerticalAlignment="Top" Name="mainStackPanel" Orientation="Vertical" Margin="5,5,5,5"></StackPanel>
    </Grid>
</UserControl>
